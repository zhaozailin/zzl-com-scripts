  "description": "react UI组件构建工具",
  "bin": {
    "com-scripts": "./bin/com-scripts.js"
  },
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "eslint": "eslint ./ --fix --format codeframe",
    "test": "echo \"Error: no test specified\" && exit 1"
